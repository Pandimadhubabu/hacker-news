{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/destructuring-assignment": [0],
        "react/prop-types": [0],
        "react/no-unescaped-entities": [0],
        "no-plusplus": [0],
        "operator-linebreak": [0],
        "max-len": [0],
        "jsx-a11y/anchor-is-valid":
            [ "error", {
                "components": [ "Link" ]
            }]
    }
}
